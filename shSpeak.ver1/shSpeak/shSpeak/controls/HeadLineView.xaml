<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="shSpeak.controls.HeadLineView">
    <ContentView.Content>
        <StackLayout Orientation="Horizontal" Padding="0" BackgroundColor="#66AFD6">
            <StackLayout HorizontalOptions="Start">
                <Button x:Name="BgEndBtn" Text="배경변경" CommandParameter="BgImg" Clicked="EndBtn_Clicked" />
            </StackLayout>
            <StackLayout HorizontalOptions="EndAndExpand">
                <Button x:Name="EndBtn" Text="사진변경" CommandParameter="UserImg" Clicked="EndBtn_Clicked" />
            </StackLayout>
        </StackLayout>
    </ContentView.Content>
</ContentView>